{
  "name": "swf-emitter",
  "version": "0.12.0",
  "description": "SWF files emitter",
  "licenses": [
    {
      "type": "AGPL-3.0-or-later",
      "url": "https://spdx.org/licenses/AGPL-3.0-or-later.html"
    }
  ],
  "keywords": [
    "swf",
    "emitter"
  ],
  "homepage": "https://github.com/open-flash/swf-emitter",
  "repository": {
    "type": "git",
    "url": "git@github.com:open-flash/swf-emitter.git"
  },
  "type": "module",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "files": [
    "./lib/"
  ],
  "exports": {
    ".": "./lib/index.js",
    "./lib/": "./lib/"
  },
  "scripts": {
    "build": "tsc --build src/lib/tsconfig.json",
    "pretest": "tsc --build src/test/tsconfig.json",
    "test": "mocha \"test/**/*.spec.js\"",
    "posttest": "yarn run lint",
    "lint": "eslint \"./src/**/*.ts\"",
    "format": "eslint --fix \"./src/**/*.ts\"",
    "prestart": "tsc --build src/main/tsconfig.json",
    "start": "node ./main/main.js"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@open-flash/stream": "^0.3.0",
    "incident": "^3.2.1",
    "semantic-types": "^0.1.1",
    "swf-types": "^0.12.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.4",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "chai": "^4.2.0",
    "eslint": "^7.8.1",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "furi": "^2.0.0",
    "kryo": "^0.11.0",
    "kryo-json": "^0.11.0",
    "mocha": "^8.1.3",
    "rimraf": "^3.0.2",
    "swf-parser": "^0.12.0",
    "typescript": "^4.0.2"
  },
  "c88": {
    "match": [
      "build/test/lib/*.{js,mjs}",
      "build/test/lib/**/*.{js,mjs}"
    ]
  }
}
